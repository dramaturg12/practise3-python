a = float (input ('введите старший кф: '))
b = float (input ('введите младший кф: '))
c = float (input ('введите свободный член: '))
D= b**2-4*a*c
if D<0: 
 print('корней нет')
if D==0:
  x=-b/(2*a)
  print('x= ',x)
if D>0:
  x1=(-b+ (D** .5))/(2*a)
  x2=(-b- (D** .5))/(2*a)
  print('x1= ', x1)
  print('x2= ', x2)
